{
  "_version": "1.59.0",
  "sap.app": {
    "id": "brahim.project",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.1",
      "toolsId": "74bef010-8e2f-47f4-8140-32d4ac8c28fd"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZRH_ODATA_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.122.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "brahim.project.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
        
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "brahim.project.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "Dashboard",
          "pattern": "Dashboard",
          "target": "Dashboard"
        },
        {
          "name": "Authentification",
          "pattern": "Authentification",
          "target":"Authentification"
        },
        {
          "name": "Test",
          "pattern": "Test",
          "target": "Test"
        },
        {
          "name": "Dashboardf",
          "pattern": "Dashboardf/{id}",
          "target": "Dashboardf"
        },
        {
          "name": "Form",
          "pattern": "Form",
          "target": "Form"
        }
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition":"flip",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home"
        },
        "Dashboard": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "Authentification": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewId": "Authentification",
          "viewName": "Authentification"
        },
        "Test": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewId": "Test",
          "viewName": "Test"
        },
        "Dashboardf": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewId": "Dashboardf",
          "viewName": "Dashboardf"
        },
        "Form": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewId": "Form",
          "viewName": "Form"
        }
      }
    },
    "rootView": {
      "viewName": "brahim.project.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
 