<mvc:View xmlns:f="sap.f"
	controllerName="brahim.project.controller.Dashboard"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	height="100%">


	<tnt:ToolPage
        id="app"
        class="sapUiDemoToolPage">
        <tnt:header>
        <tnt:ToolHeader id="shellLikeToolHeader" class="sapUiTinyMargin">
			<Button icon="sap-icon://menu2" type="Transparent" tooltip="Menu" press=".onCollapseExpandPress">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Button>
			<Image src="../utils/images/aymax.png" decorative="false" tooltip="AYMAX Logo">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Image>
			<Text text="Your Business Powered By IT !" wrapping="false" id="secondTitle">
				<layoutData>
					<OverflowToolbarLayoutData priority="Disappear"/>
				</layoutData>
			</Text>
			<ToolbarSpacer />
			<ToolbarSpacer id="spacerId">
				<layoutData>
					<OverflowToolbarLayoutData priority="Low" group="1"/>
				</layoutData>
			</ToolbarSpacer>
			<Button visible="false" icon="sap-icon://search" type="Transparent" id="searchButton" tooltip="Search"/>
			<OverflowToolbarButton icon="sap-icon://source-code" type="Transparent" tooltip="Action 1" text="Action 1">
				<layoutData>
					<OverflowToolbarLayoutData group="2"/>
				</layoutData>
			</OverflowToolbarButton>
			<OverflowToolbarButton icon="sap-icon://card" type="Transparent" tooltip="Action 2" text="Action 2">
				<layoutData>
					<OverflowToolbarLayoutData group="2"/>
				</layoutData>

			</OverflowToolbarButton>
			<ToolbarSeparator>
				<layoutData>
					<OverflowToolbarLayoutData group="2"/>
				</layoutData>
			</ToolbarSeparator>
			<OverflowToolbarButton icon="sap-icon://action-settings" type="Transparent" text="Settings"/>
			<Button icon="sap-icon://bell" type="Transparent" tooltip="Notification">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Button>
			<tnt:ToolHeaderUtilitySeparator/>
			<ToolbarSpacer width="1.125rem"/>
			<Avatar src="test-resources/sap/tnt/images/Woman_avatar_01.png" displaySize="XS" press=".onAvatarPressed" tooltip="Profile">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Avatar>
			</tnt:ToolHeader>
        </tnt:header>
        <tnt:sideContent>
           <tnt:SideNavigation id="sideNavigation" selectedKey="walked" width="350px">
				<tnt:NavigationList >
					<tnt:NavigationListItem class="sideNavigation" text="Home" icon="sap-icon://home" />
					<tnt:NavigationListGroup text="New">
						<tnt:NavigationListItem class="sapMText sapTntNLIText sapMTextNoWrap" text="Performance Pulse" icon="sap-icon://performance">
							<tnt:NavigationListItem text="Auto Evaluation" icon="sap-icon://form"/>
							<tnt:NavigationListItem text="Evaluation Manager" icon="sap-icon://manager"/>
							<tnt:NavigationListItem text="Fixation Des Objectifs" icon="sap-icon://goal" />
							<tnt:NavigationListItem text="Notes KPI" icon="sap-icon://kpi-corporate-performance"/>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="Engagement Pulse" icon="sap-icon://curriculum">
							<tnt:NavigationListItem text="Formulaire" icon="sap-icon://form" />
							<tnt:NavigationListItem text="Chartes Analytiques" icon="sap-icon://filter-analytics"/>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="Revue Des Talents" icon="sap-icon://favorite">
							<tnt:NavigationListItem text="9 Box Grid" icon="sap-icon://grid"/>
							<tnt:NavigationListItem text="Risque De Départ" icon="sap-icon://offsite-work"/>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="Matrice Des Compétences" icon="sap-icon://table-view">
							<tnt:NavigationListItem text="Technique" icon="sap-icon://employee" />
							<tnt:NavigationListItem text="Fonctionelle" icon="sap-icon://employee"/>
							<tnt:NavigationListItem text="Basis" icon="sap-icon://employee"/>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="Carriére" icon="sap-icon://mileage">
							<tnt:NavigationListItem text="Revenu Salarial" icon="sap-icon://money-bills"/>
							<tnt:NavigationListItem text="Evaluation Des Compétences" icon="sap-icon://badge"/>
							<tnt:NavigationListItem text="Formation Et Développement" icon="sap-icon://study-leave"/>
							<tnt:NavigationListItem text="Promotions Et Récompenses" icon="sap-icon://competitor"/>
							<tnt:NavigationListItem text="Bilan De Carrière" icon="sap-icon://doc-attachment"/>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="My Moment" icon="sap-icon://ai">
							<tnt:NavigationListItem text="Evènements Et Activités" icon="sap-icon://widgets"/>
							<tnt:NavigationListItem text="Défis Amicaux" icon="sap-icon://puzzle"/>
							<tnt:NavigationListItem text="Programmes De Bien Être" icon="sap-icon://physical-activity"/>
							<tnt:NavigationListItem text="Feedbacks Et Suggestions" icon="sap-icon://feedback"/>
						</tnt:NavigationListItem>

					</tnt:NavigationListGroup>
				</tnt:NavigationList>

				<tnt:fixedItem>
					<tnt:NavigationList>
						<tnt:NavigationListItem selectable="false" href="https://sap.com" target="_blank" text="Déconnexion" icon="sap-icon://journey-depart"/>
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>

        </tnt:sideContent>
        <tnt:mainContents>
 
            <VBox class="sapUiSmallMargin">
                <f:SimpleForm id="SimpleFormToolbar"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    labelSpanXL="4"
                    labelSpanL="3"
                    labelSpanM="4"
                    labelSpanS="12"
                    adjustLabelSpan="false"
                    emptySpanXL="0"
                    emptySpanL="4"
                    emptySpanM="0"
                    emptySpanS="0"
                    columnsXL="2"
                    columnsL="1"
                    columnsM="1"
                    singleContainerFullSize="false"
                    ariaLabelledBy="Title1" >
                    <f:toolbar>
                        <Toolbar id="TB1">
                            <Text class="evalTitle" text="Auto-évaluation des performances Q1 -2024"/>
                            <ToolbarSpacer />
                            <Button icon="sap-icon://settings"/>
                            <Button icon="sap-icon://drop-down-list" />
                        </Toolbar>
                    </f:toolbar>
                    <f:content>
                        <Toolbar ariaLabelledBy="Title2">
                        <Title id="Title11" class="number" text="1"/>
                        <Text class="number" text="Informations Personnels"></Text>
                            <!-- <Title id="Title2" class="number" text="Personal informations"/> -->
                            <ToolbarSpacer />
                            <Button icon="sap-icon://settings"/>
                        </Toolbar>
                       
                        <Label text="Name" />
                        <Input class="in" value="Farah" />
                        <Label text="Street/No." />
                        <Input value="{Street}">
                        </Input>
                        <Input value="{HouseNumber}">
                            <layoutData>
                                <l:GridData span="XL2 L1 M3 S4" />
                            </layoutData>
                        </Input>
                        <Label text="ZIP Code/City" />
                        <Input value="{ZIPCode}">
                            <layoutData>
                                <l:GridData span="XL2 L1 M3 S4" />
                            </layoutData>
                        </Input>
                        <Input value="{City}" />
                        <Label text="Country" />
                        <Select id="country" selectedKey="{Country}">
                            <items>
                                <core:Item text="England" key="England"/>
                                <core:Item text="Germany" key="Germany"/>
                                <core:Item text="USA" key="USA"/>
                            </items>
                        </Select>
                        <Label text="Web" />
                        <Input value="{Url}" type="Url" />
                        <Label text="Twitter" />
                        <Input value="{Twitter}" />
                        <Label text="Web" />
                        <Input value="{Url}" type="Url" />
                        <Label text="Twitter" />
                        <Input value="{Twitter}" />
       
                   
                        <Toolbar ariaLabelledBy="Title3">
                            <Title id="Title3" text="2"/>
                            <Text id="Title33" text="Questionnaire de Performances"/>
                            <ToolbarSpacer />
                            <Button icon="sap-icon://settings"/>
                        </Toolbar>
                        <Label text="Web" />
                        <Input value="{Url}" type="Url" />
                        <Label text="Twitter" />
                        <Input value="{Twitter}" />
                    </f:content>
                </f:SimpleForm>
            </VBox>
        </tnt:mainContents>
	</tnt:ToolPage>
</mvc:View>