<mvc:View
	controllerName="brahim.project.controller.Dashboardf"
	xmlns="sap.m"
    xmlns:card="sap.f.cards"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:tnt="sap.tnt"
	height="100%">
	
<tnt:ToolPage
		id="app"
		class="sapUiDemoToolPage">
		<tnt:header>
			<tnt:ToolHeader>
				<Button
					id="sideNavigationToggleButton"
					icon="sap-icon://menu2"
					type="Transparent"
					press=".onSideNavButtonPress"
					tooltip="{i18n>collpaseMenuButtonText}">
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
				<ToolbarSpacer/>
				<Title
					text="{i18n>appTitle}"
					level="H2"/>
				<ToolbarSpacer/>
				<Button
					id="errorButton"
					icon="sap-icon://message-popup"
				>
				 <!-- This can be added to button props 
					visible="{= ${alerts>/alerts/errors}.length === 0 ? false : true }"
					type="Transparent"
					press=".onMessagePopoverPress"
					tooltip="{i18n>errorButtonTooltip}" -->
					<layoutData>
						<OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
					</layoutData>
				</Button>
				<Button
					id="notificationButton"
					icon="sap-icon://ui-notifications"
				>
				 <!-- This can be added to button props 
					visible="{= ${alerts>/alerts/notifications}.length === 0 ? false : true }"
					type="Transparent"
					press=".onNotificationPress"
					tooltip="{i18n>notificationButtonTooltip}" -->	
					<layoutData>
						<OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
					</layoutData>
				</Button>
				<Button
					id="userButton"
					text="{i18n>userName}"
					type="Transparent"
					press=".onUserNamePress">
					<layoutData>
						<OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
					</layoutData>
				</Button>
			</tnt:ToolHeader>
		</tnt:header>
		<tnt:sideContent>
			<tnt:SideNavigation
				expanded="true"
				selectedKey="{side>/selectedKey}">
				<tnt:NavigationList
					items="{
						path: 'side>/navigation',
						templateShareable: false
					}">
					<tnt:NavigationListItem
						text="{path: 'side>titleI18nKey', formatter: '.getBundleText'}"
						icon="{side>icon}"
						expanded="{side>expanded}"
						items="{
							path: 'side>items',
							templateShareable: false
						}"
						key="{side>key}"
						href="#/{side>key}">
						<tnt:NavigationListItem
							text="{path: 'side>titleI18nKey', formatter: '.getBundleText'}"
							key="{side>key}"
							href="#/{side>key}">
						</tnt:NavigationListItem>
					</tnt:NavigationListItem>
				</tnt:NavigationList>
				<tnt:fixedItem>
					<tnt:NavigationList
						items="{
							path: 'side>/fixedNavigation',
							templateShareable: false
						}">
						<tnt:NavigationListItem
							text="{path: 'side>titleI18nKey', formatter: '.getBundleText'}"
							icon="{side>icon}"
							key="{side>key}"
							href="#/{side>key}">
						</tnt:NavigationListItem>
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>

			<VBox class="sapUiSmallMargin">
				<f:SimpleForm id="SimpleFormToolbar"
					editable="true"
					layout="ResponsiveGridLayout"
					labelSpanXL="4"
					labelSpanL="3"
					labelSpanM="4"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="4"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false"
					ariaLabelledBy="Title1" >
					<f:toolbar>
						<Toolbar id="TB1">
							<Text id="Title1" text="Auto-Ã©valuation des performances"/>
							<ToolbarSpacer />
							<Button icon="sap-icon://settings"/>
							<Button icon="sap-icon://drop-down-list" />
						</Toolbar>
					</f:toolbar>
					<f:content>
						<Toolbar ariaLabelledBy="Title2">
						<Title id="Title11" class="number" text="1"/>
						<Text class="number" text="Personal Informations"></Text>
							<!-- <Title id="Title2" class="number" text="Personal informations"/> -->
							<ToolbarSpacer />
							<Button icon="sap-icon://settings"/>
						</Toolbar>
						<Label text="Name" />
						<Input class="in" value="Eya Belghith" />
						<Label text="Street/No." />
						<Input value="{Street}">
						</Input>
						<Input value="{HouseNumber}">
							<layoutData>
								<l:GridData span="XL2 L1 M3 S4" />
							</layoutData>
						</Input>
						<Label text="ZIP Code/City" />
						<Input value="{ZIPCode}">
							<layoutData>
								<l:GridData span="XL2 L1 M3 S4" />
							</layoutData>
						</Input>
						<Input value="{City}" />
						<Label text="Country" />
						<Select id="country" selectedKey="{Country}">
							<items>
								<core:Item text="England" key="England"/>
								<core:Item text="Germany" key="Germany"/>
								<core:Item text="USA" key="USA"/>
							</items>
						</Select>
						<Label text="Web" />
						<Input value="{Url}" type="Url" />
						<Label text="Twitter" />
						<Input value="{Twitter}" />
						<Label text="Web" />
						<Input value="{Url}" type="Url" />
						<Label text="Twitter" />
						<Input value="{Twitter}" />
		
					
						<Toolbar ariaLabelledBy="Title3">
							<Title id="Title3" text="2"/>
							<Text id="Title33" text="Evaluez vous"/>
							<ToolbarSpacer />
							<Button icon="sap-icon://settings"/>
						</Toolbar>
						<Label text="Web" />
						<Input value="{Url}" type="Url" />
						<Label text="Twitter" />
						<Input value="{Twitter}" />
					</f:content>
				</f:SimpleForm>
			</VBox>

		</tnt:mainContents>
</tnt:ToolPage>
</mvc:View>